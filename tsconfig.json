{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "@envs/*": ["envs/*"],
      "@assets/*": ["src/assets/*"],
      "@data/*": ["src/data/*"],
      "@domain/*": ["src/domain/*"],
      "@presentation/*": ["src/presentation/*"],
      "@utils/*": ["src/utils/*"],
    }
  }
}
